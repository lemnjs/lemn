{
  "name": "lemn",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "eslint": "^5.12.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mkdirp": "^0.5.1",
    "nyc": "^13.1.0",
    "rollup": "^1.0.1",
    "smokestack": "^3.6.0",
    "standard-version": "^4.4.0",
    "tap-closer": "^1.0.0",
    "tap-finished": "^0.0.1",
    "tape": "^4.9.2",
    "tape-run": "^5.0.0",
    "terser": "^3.14.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "description": "",
  "files": [
    "index.js",
    "src/*.js"
  ],
  "directories": {},
  "scripts": {
    "build": "npm run build:lite && npm run build:full && npm run build:examples && npm run build:docs",
    "build:lite": "mkdirp dist && rollup -m -f iife -n lemn index.js --file dist/web.js && terser --mangle --compress --output dist/web.min.js --source-map < dist/web.js",
    "build:full": "mkdirp dist && rollup -m -f iife -n lemn lite.js --file dist/web.lite.js && terser --mangle --compress --output dist/web.lite.min.js --source-map < dist/web.lite.js",
    "build:examples": "NODE_ENV=production webpack --config examples/webpack.config.js",
    "build:docs": "jsdoc src -d dist/docs",
    "test": "NODE_ENV=test WEBPACK_STDOUT=1 webpack --config build/webpack.config.js --entry ./tests/index | tape-run",
    "test:build:file": "NODE_ENV=test webpack --config build/webpack.config.js --entry ./tests/index --output-path tmp/test --output-filename main.js",
    "test:build:dll": "webpack --config tests/webpack.config.dll.js",
    "test:dll": "NODE_ENV=test WEBPACK_STDOUT=1 WEBPACK_USE_TEST_DLL=1 webpack --config build/webpack.config.js --entry ./tests/index | cat tmp/test/dll.js - | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzgoddard/-.git"
  },
  "keywords": [],
  "author": "Michael 'Z' Goddard <mzgoddard@gmail.com>",
  "bugs": {
    "url": "https://github.com/mzgoddard/-/issues"
  },
  "homepage": "https://github.com/mzgoddard/-#readme"
}
